n = 1
S = 0
tickets = int(input("Введите количество билетов: "))
if tickets <= 0 : # проверка введенного числа билетов на логичность - не меньше нуля и не отрицательное целое число
    print("Введите верное количество билетов!")
    tickets = int(input("Введите количество билетов: "))
for n in range (1, tickets+1):
    age = int(input("Сколько лет посетителю?"))
    if age < 0 or age > 100:# проверка введенного возраста на логичность - не меньше нуля и не отрицательное целое число. Груднички допускаются))
        print("Введите верный возраст!")
        age = int(input("Сколько лет посетителю?"))
    if age < 18 :
        S = S + 0
    elif 18 <= age < 25 :
         S = S + 999
    else: 
         S = S + 1390
    n += 1
Result = "Сумма к оплате - %d .руб" %(S)
print(Result)
